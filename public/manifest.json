{
  "manifest_version": 3,
  "name": "Snap",
  "version": "1.0",
  "chrome_style": false,
  "description": "Fill out forms with a snap.",
  "action": {
    "default_popup": "index.html",
    "default_ title": "Snap"
  },
  "permissions": ["scripting", "tabs", "activeTab", "clipboardWrite"],
  "host_permissions": ["https://*/*", "http://*/*"],
  "icons": {
    "16": "assets/logo.png",
    "32": "assets/logo.png",
    "128": "assets/logo.png"
  }
}
